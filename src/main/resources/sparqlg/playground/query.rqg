PREFIX fun: <http://w3id.org/sparql-generate/fn/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>

GENERATE {
    <{?id}> a foaf:Person ;
        rdfs:label ?name .
}
WHERE {
   BIND(fun:JSONPath(?body, '$.id' ) AS ?id)
   BIND(fun:JSONPath(?body, '$.name' ) AS ?name)
}
